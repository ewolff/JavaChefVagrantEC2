{
  "json_class": "Chef::Role",
  "name": "tomcatserver",
  "description": "Install Tomcat and a web application inside Tomcat",
  "default_attributes": {
    "webapp": {
      "webapp": "demo.war"
    },
    "tomcat": {
      "port" : 8080
    }
  },
  "chef_type": "role",
